#!/bin/csh -f
########################################################################
#                                                                      #
# This file has been automatically generated by "gmake"                #
# using settings in reduce/src/Makefile                                #
#                                                                      #
########################################################################
 
cd `dirname $0`
 
set JAVA=java -Dgnu.io.rxtx.SerialPorts=/dev/filterwheel
 
if( $?UDRIVERPATH ) then
  set UDRIVERPATH=../lib/usdriver.jar:$UDRIVERPATH
else
  set UDRIVERPATH=../lib/usdriver.jar
endif
 
if( $#argv == 1 ) then
   ${JAVA} -DCONFIG_FILE=$1 -classpath ${UDRIVERPATH} warwick.marsh.ultracam.usdriver.Usdriver
else
  ${JAVA} -DCONFIG_FILE=usdriver.conf -classpath ${UDRIVERPATH} warwick.marsh.ultracam.usdriver.Usdriver
endif
 
exit
